            <tt:p xml:id="sub0" style="textCenter" region="bottom" begin="00:00:04.200" end="00:00:06.120">
                <tt:span style="textWhite">Diese Untertitel</tt:span>
                <tt:br />
                <tt:span style="textWhite">sind live produziert.</tt:span>
            </tt:p>
            <tt:p xml:id="sub1" style="textCenter" region="bottom" begin="00:00:02.120" end="00:00:05.440">
                <tt:span style="textWhite">Willkommen zu unserer Sendung.</tt:span>
            </tt:p>
            <tt:p xml:id="sub2" style="textCenter" region="bottom" begin="00:00:05.560" end="00:00:09.280">
                <tt:span style="textWhite">Die Welt redet über</tt:span>
                <tt:br />
                <tt:span style="textWhite">einen neuen Friedensplan für Gaza.</tt:span>
            </tt:p>
            <tt:p xml:id="sub3" style="textCenter" region="bottom" begin="00:00:09.360" end="00:00:12.000">
                <tt:span style="textWhite">Wir alle haben die Bilder</tt:span>
                <tt:br />
                <tt:span style="textWhite">aus Washington gesehen.</tt:span>
            </tt:p>
            <tt:p xml:id="sub4" style="textCenter" region="bottom" begin="00:00:12.080" end="00:00:16.440">
                <tt:span style="textWhite">Trump, der von</tt:span>
                <tt:br />
                <tt:span style="textWhite">einem historischen Tag sprach,</tt:span>
            </tt:p>
            <tt:p xml:id="sub1132" style="textCenter" region="bottom" begin="01:17:39.760" end="01:17:42.280">
